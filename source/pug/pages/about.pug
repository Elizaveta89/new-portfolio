extends ../layout
include ../sections/_mixins.pug



block title
    title About

block body
    block preloader
        include ../sections/preloader.pug
    .wrapper.wrapper__about
        .maincontent

            block header__about
                include ../sections/header.pug

            block content
                .content__about
                    .about__left
                        .about__left-title.general__title
                            span Обо мне
                        .about__left-info
                            .about__left-foto
                                img.about__left-img(src="./assets/img/avatar/me.jpg" alt="Аватар")
                            .general__title.about__left-who
                                span Кто я
                            .about__text
                                p Я веб разработчик из Новосибирска. Мне 27 лет. Я занимаюсь разработкой современных сайтов и приложений. Мне нравится делать интересные и современные проекты.
                                p Этот сайт я сделал в рамках обучения в Школе онлайн образования LoftSchool. Чуть позже я освежу в нём свой контент. А пока посмотрите,как тут всё классно и красиво!
                    .about__right
                        .about__right-info
                            .general__title.about__right-title
                                span Чем я могу быть вам полезна
                            .about__text.about__right-text
                                p Больше всего меня привлекет Frontend разработка, но я также знаком и могу решать не сложные задачина Backend. Но давайте по порядку.
                            .skills
                                .skills__block
                                    .skills__title Frontend
                                    ul.skills__list
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green(r="16" cx="16" cy="16" stroke-dasharray="90 100")
                                                .skills__circle-text HTML5
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green(r="16" cx="16" cy="16" stroke-dasharray="80 100")
                                                .skills__circle-text CSS3
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green(r="16" cx="16" cy="16" stroke-dasharray="80 100")
                                                .skills__circle-text JavaScript & Jquery
                                .skills__block
                                    .skills__title Backend
                                    ul.skills__list
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green.skills__circle-php(r="16" cx="16" cy="16" stroke-dasharray="40 100")
                                                .skills__circle-text PHP
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green.skills__circle-mySQL(r="16" cx="16" cy="16" stroke-dasharray="55 100")
                                                .skills__circle-text mySQL
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green.skills__circle-php(r="16" cx="16" cy="16" stroke-dasharray="30 100")
                                                .skills__circle-text Node.js & npm
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green.skills__circle-Mongo(r="16" cx="16" cy="16" stroke-dasharray="60 100")
                                                .skills__circle-text Mongo.db
                                .skills__block
                                    .skills__title WorkFlow
                                    ul.skills__list
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green(r="16" cx="16" cy="16" stroke-dasharray="85 100")
                                                .skills__circle-text Git
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green.skills__circle-Gulp(r="16" cx="16" cy="16" stroke-dasharray="65 100")
                                                .skills__circle-text Gulp
                                        li.skills__item
                                            svg(viewBox="0 0 32 32")
                                                circle.skills__circle-grey(r="16" cx="16" cy="16")
                                                circle.skills__circle-green.skills__circle-Mongo(r="16" cx="16" cy="16" stroke-dasharray="55 100")
                                                .skills__circle-text Bower
                .map(id="map")
                .contacts__window
                    .general__title.contacts__title
                        span Контакты
                    ul.contacts__list
                        li.contacts__item
                            a.contacts__link(href='skype:stopangina?chat')
                                img.contacts__item-pic(src="./assets/img/icons/skype.png" alt="Скайп")
                                span.contacts__item-text stopangina
                        li.contacts__item
                            a.contacts__link(href="mailto:eovinova89@gmail.ru")
                                img.contacts__item-pic(src="./assets/img/icons/envelope.png" alt="Почта")
                                span.contacts__item-text eovinova89@gmail.com
                        li.contacts__item
                            a.contacts__link(href="#")
                                img.contacts__item-pic(src="./assets/img/icons/phone.png" alt="Телефон")
                                span.contacts__item-text +7 906 727 38 31
                        li.contacts__item
                            a.contacts__link(href="https://www.google.ru/maps/place/%D0%91%D1%80%D0%B0%D1%82%D0%B8%D1%81%D0%BB%D0%B0%D0%B2%D1%81%D0%BA%D0%B0%D1%8F/@55.6590248,37.7475688,17z/data=!3m1!4b1!4m5!3m4!1s0x414ab41165046341:0xc3d26635102a6507!8m2!3d55.6590248!4d37.7497575" target="_blank")
                                img.contacts__item-pic(src="./assets/img/icons/map_marker.png" alt="Маркер")
                                span.contacts__item-text Москва, м. Братиславская
    block footer
        include ../sections/footer.pug